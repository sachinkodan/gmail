@charset "utf-8";

* { margin: 0px; }

html { height: 100%; }

body { font-family: Helvetica, Arial; height: 100%; }

.bgBox { background-color: rgb(249, 249, 249); padding: 50px; border-radius: 5px; border: 2px solid rgb(222, 222, 222); max-width: 430px; margin: 0px auto 50px; box-sizing: border-box; }

.bgBox h2 { margin-top: 0px; font-size: 23px; color: rgb(72, 72, 72); text-align: center; margin-bottom: 41px; font-weight: bold; }

.bgBox input { border-radius: 3px; transition: none 0s ease 0s; height: 47px; padding: 0px 15px; box-shadow: none; font-size: 15px; line-height: 16px; border: 1px solid rgb(222, 222, 222); font-weight: normal; color: rgb(72, 72, 72); width: 100%; outline: none; box-sizing: border-box; }

.bgBox input[type="submit"] { height: 50px; font-size: 21px; }

.bgBox input.valid_error { border-color: rgb(232, 50, 46); }

.bgBox .inputRow { padding: 6px 0px; margin-bottom: 8px; overflow: auto; }

.bgBox .inputRow.submit { margin-top: 10px; }

.bgBox .inputRow .signIn { color: rgb(255, 255, 255); background: rgb(87, 149, 210); border-color: rgb(87, 149, 210); font-weight: bold; cursor: pointer; }

.bgBox .inputRow .signIn:hover { background: rgb(60, 120, 179); }

.bgBox .inputRow .signIn.loading { background: url("preloader.gif") center center no-repeat rgb(87, 149, 210); text-indent: -10000px; }

.loginForm { position: relative; top: 50%; margin-top: -215px; }

.error { text-align: center; margin-top: 20px; }

span { clear: both; font-size: 12px; line-height: 8px; padding: 8px 0px 0px; text-align: center; color: rgb(232, 50, 46) !important; }

@media (max-width: 430px) {
  .bgBox { width: 100%; height: 100%; border: none; }
  .loginForm { top: 0px; margin: 0px; }
  .bgBox h2 { margin-bottom: 20px; }
}

@media (max-width: 350px) {
  .bgBox { padding: 30px; }
  .bgBox h2 { font-size: 20px; }
}

@media (max-width: 250px) {
  .bgBox { min-width: 250px; }
}
